{
  "meta": {
    "id": "github-dark",
    "name": "GitHub Dark",
    "version": "2.0.0",
    "author": "Tanmark",
    "description": "GitHub 风格的暗色主题",
    "type": "dark",
    "tags": ["dark", "minimal", "modern", "github"],
    "previewColor": "#4a9eff"
  },
  "colors": {
    "bg": "#1e1e1e",
    "bgSecondary": "#252526",
    "sidebarBg": "#252526",
    "fg": "#cccccc",
    "fgSecondary": "#9d9d9f",
    "fgMuted": "#6e7681",
    "fgOnDark": "#ffffff",
    "border": "#3e3e42",
    "borderLight": "#30363d",
    "divider": "#30363d",
    "hover": "#2a2d2e",
    "active": "#37373d",
    "focus": "#4a9eff",
    "accent": "#4a9eff",
    "accentHover": "#58a6ff",
    "success": "#3fb950",
    "warning": "#d29922",
    "error": "#f85149",
    "info": "#58a6ff",
    "editor": {
      "text": "#cccccc",
      "heading": "#e0e0e0",
      "h1": "#e0e0e0",
      "h2": "#cccccc",
      "h3": "#cccccc",
      "link": "#58a6ff",
      "linkHover": "#79c0ff",
      "codeBg": "#2d2d30",
      "codeColor": "#cccccc",
      "codeBorder": "#3e3e42",
      "codeblockBg": "#161b22",
      "codeblockText": "#e6edf3",
      "blockquoteBorder": "#4a9eff",
      "blockquoteBg": "#2d2d30",
      "blockquoteText": "#9d9d9d",
      "tableBorder": "#3e3e42",
      "tableHeaderBg": "#2d2d30",
      "tableHeaderText": "#e0e0e0",
      "tableRowEvenBg": "#252526",
      "tableRowHoverBg": "#2a2d2e",
      "hrColor": "#3e3e42",
      "listMarkerColor": "#58a6ff",
      "taskCheckboxBorder": "#3e3e42",
      "taskCheckboxChecked": "#4a9eff",
      "selectionBg": "#58a6ff",
      "selectionColor": "#ffffff",
      "taskCheckboxHoverBg": "#2d2d30",
      "taskCheckboxHoverBorder": "#58a6ff",
      "taskCompletedText": "#8b949e",
      "codeblockBorder": "rgba(255, 255, 255, 0.1)",
      "codeblockShadow": "0 2px 8px rgba(0, 0, 0, 0.4)"
    }
  },
  "typography": {
    "fontSize": 16,
    "lineHeight": 1.75,
    "fontFamily": "-apple-system, BlinkMacSystemFont, 'Segoe UI', 'Roboto', 'Helvetica', 'Arial', sans-serif",
    "codeFontFamily": "'Monaco', 'Menlo', 'Ubuntu Mono', 'Consolas', monospace",
    "headingFontFamily": "-apple-system, BlinkMacSystemFont, 'Segoe UI', 'Roboto', 'Helvetica', 'Arial', sans-serif",
    "headingFontWeight": 600,
    "h1Size": 2.25,
    "h2Size": 1.875,
    "h3Size": 1.5,
    "h4Size": 1.25,
    "h5Size": 1.125,
    "h6Size": 1,
    "maxWidth": 900,
    "padding": 40
  },
  "ui": {
    "borderRadiusSm": 4,
    "borderRadiusMd": 8,
    "borderRadiusLg": 12,
    "borderRadius": 6,
    "buttonPadding": "8px 16px",
    "inputPadding": "8px 12px",
    "transitionFast": "0.15s",
    "transitionNormal": "0.2s",
    "transitionSlow": "0.3s",
    "transitionSpeed": "0.2s",
    "shadowSm": "0 1px 2px rgba(0, 0, 0, 0.3)",
    "shadowMd": "0 4px 6px rgba(0, 0, 0, 0.4)",
    "shadowLg": "0 10px 15px rgba(0, 0, 0, 0.5)",
    "scrollbarWidth": 8,
    "scrollbarTrack": "transparent",
    "scrollbarThumb": "rgba(255, 255, 255, 0.2)",
    "scrollbarThumbHover": "rgba(255, 255, 255, 0.3)",
    "scrollbarRadius": 4,
    "resizerBg": "#484f58",
    "resizerHoverBg": "#6e7681",
    "emptyStateColor": "#6e7681",
    "outlineFontSize": "0.8125rem",
    "outlineListPadding": "0.5rem 0",
    "outlineItemPaddingY": "0.3rem",
    "outlineItemPaddingX": "0.5rem",
    "outlineIndentStep": "12px",
    "outlineEmptyPadding": "0.75rem 0.75rem",
    "outlineEmptyFontSize": "0.75rem"
  }
}
